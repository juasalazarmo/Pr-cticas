      "source": [
        "#Ejemplo de señal despues de cero y pendiente y discretización\n",
        "#Recuerde que el proceso de discretización debe ser implementado\n",
        "from scipy.spatial.distance import cdist\n",
        "\n",
        "#señal original\n",
        "import math\n",
        "import numpy as np\n",
        "\n",
        "#Definimos las frecuencias\n",
        "F1 = (100/(2*math.pi))\n",
        "F2 = (20/(2*math.pi))\n",
        "F3 = (300/(2*math.pi))\n",
        "# Definimos los periodos de cada onda\n",
        "T1 = 1/F1\n",
        "T2 = 1/F2\n",
        "T3 = 1/F3\n",
        "#Definimos el periodo total\n",
        "Tt = (math.pi/10)\n",
        "#Definimos la amplitud de cada onda\n",
        "A1 = 8\n",
        "A2 = 10\n",
        "A3 = 7\n",
        "#Definimos la frecuencia de muestreo (de la onda de mayor velocidad)\n",
        "Fs = 10*F3\n",
        "#Definimos el periodo de muestreo\n",
        "Ts = 1/Fs\n",
        "#Definimos la variable tiempo con la función arange\n",
        "tv = np.arange(0,2*Tt,Ts)\n",
        "#Definimos las ondas a discretizar\n",
        "Y1 = A1*np.cos(100*tv)\n",
        "Y2 = A2*np.sin(20*tv)\n",
        "Y3 = A1*np.cos(300*tv-(math.pi/7))\n",
        "y = Y1 + Y2 - Y3\n",
        "\n",
        "#Definimos el cero y la pendiente de la gráfica\n",
        "ycs = my_ceropen(y, ymin=0, ymax=5)\n",
        "\n",
        "#Número de bits y cuantización\n",
        "nbits = 3\n",
        "rmin = 0\n",
        "rmax = 5\n",
        "ve = np.linspace(rmin, rmax, 2**nbits)\n",
        "#Distancias y cero pendiente a vector cuantización\n",
        "d = cdist(ycs.reshape(-1,1),ve.reshape(-1,1))\n",
        "d.shape"
      ],
